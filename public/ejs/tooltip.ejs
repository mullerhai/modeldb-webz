<div class="tooltip">
  <div class="tooltip-keys">
    <% for (var i=0; i<keys.length; i++) { %>
      <div class="tooltip-key"><%= keys[i] %>:</div>
    <% } %>
  </div>

  <div class="tooltip-values">
    <% for (var i=0; i<values.length; i++) { %>
      <div class="tooltip-value"><%= values[i] %></div>
    <% } %>
  </div>

</div>